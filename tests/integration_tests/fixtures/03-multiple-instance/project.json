{
    "settings": {
        "name": "test project",
        "components_folder": "components",
        "required_plugins": []
    },
    "sources": [],
    "includes": [],
    "components": [
        "foo"
    ],
    "instances": {
        "foo1": "foo",
        "foo2": "foo"
    },
    "types": {
        "uint32_t": {
            "pass_semantic": "value",
            "defined_in": "<stdint.h>",
            "default_value": "0u"
        }
    },
    "runtime": {
        "runnables": {
            "OnInit": [
                [
                    "foo1/Runnable",
                    {
                        "arguments": {
                            "arg1": 5
                        }
                    }
                ],
                [
                    "foo2/Runnable",
                    {
                        "arguments": {
                            "arg1": 6
                        }
                    }
                ]
            ]
        },
        "port_connections": [
            {
                "provider": "foo1/WPort",
                "consumer": "foo2/RPort"
            },
            {
                "provider": "foo2/WPort",
                "consumer": "foo1/RPort"
            },
            {
                "provider": "foo1/WIPort",
                "consumers": [
                    "foo2/RIPort",
                    ["foo2/RPortFromI", {"index": 2}]
                ]
            },
            {
                "provider": "foo2/WIPort",
                "consumers": [
                    "foo1/RIPort",
                    ["foo1/RPortFromI", {"index": 3}]
                ]
            }
        ]
    }
